<div id="galerie-animata" class="galerie-a">
  <% for (let imag of locals.imagini) {
    %>
  // etapa_5: galerie statica
  <figure>
    <picture>
      <source srcset="<%- imag.fisier_mediu %>" media="(max-width:1000px)" />
      <img src="<%- imag.cale_fisier %>" alt="imagine" />
    </picture>
    <figcaption><%- imag.text_descriere %></figcaption>
  </figure>
  <% } %>
</div>